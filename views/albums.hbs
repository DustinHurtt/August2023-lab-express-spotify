{{#each results}}

    <div>

        {{#if images.1.url}}
        
                <img src={{images.1.url}} alt="artist">

            {{else}}

                <p>No image found</p>

        {{/if}}

        <h3>{{name}}</h3>

        <a href="/tracks/{{id}}">
            <button>View Tracks</button>
        </a>

        <br>

    </div>


{{/each}}